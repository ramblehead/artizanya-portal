{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "lib": ["es2017", "esnext.asynciterable"],
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules"
  ]
}
